/*# sourceMappingURL=data:application/json;base64, */</style><style>.topnav-container {
    /* border: 2px solid red; */
    height: 5em;
    display: flex;
    place-items: center;
    justify-content: space-between;
    padding: 2em;
}

.logo-container {
    /* border: 2px solid yellow; */
    width: 6em;
    display: flex;
    place-items: center;
}

.logo-container img {
    width: 100%;
}

.topnav-links {
    display: flex;
    align-items: center;
}

.topnav-links ul {
    display: flex;
    list-style-type: none;
    margin-right: 2em;
}

.topnav-links ul li {
    display: flex;
    flex-direction: column;
    margin: 0px 10px;
    overflow: hidden;
    transition: .1s;
    font-size: 18px;
}

.topnav-links ul li::after {
    content: " ";
    width: 100%;
    height: 3px;
    background-color: var(--button-color);
    transition: .5s;
    transform: translateX(-90px);
}

.topnav-links ul li:hover {
    transform: scale(1.1);
}

.topnav-links ul li:hover::after {
    transform: translateX(0px);
}

.navlink {
    text-decoration: none;
    color: var(--text-gray);
    font-weight: 700;
    transition: .5s;
}

.navlink:hover {
    /* transform: translateX(20px); */
    /* color: red; */
}

.topnav-buttons {
    display: flex;
}
